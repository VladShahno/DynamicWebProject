DROP TABLE books.books;
CREATE TABLE books.books
(
    id integer NOT NULL,
    name character(250) COLLATE pg_catalog."default" NOT NULL,
    CONSTRAINT "BOOKS_pkey" PRIMARY KEY (id)
)
WITH (
    OIDS = FALSE
    )

TABLESPACE pg_default;

ALTER TABLE books.books
    OWNER to postgres;
    
    delete from books.books;
    
insert into books.books(id, name) values (0, 'Harry Potter 1');

insert into books.books(id, name) values (1, 'Harry Potter 2');